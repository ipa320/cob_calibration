<?xml version="1.0"?>
<launch>

    <!-- run stereo calibration of left and right camera -->
    <node type="generate_config.py" pkg="cob_robot_calibration" name="$(anon robot_calibration)" output="screen">

        <rosparam file="$(find cob_robot_config)/$(env ROBOT)/user_defined/calibration_pattern.yaml"/>



    </node>
    <param name="output_system" value="$(find cob_robot_config)/$(env ROBOT)/autogenerated/system.yaml"/>
    <param name="free_system" value="$(find cob_robot_config)/$(env ROBOT)/modify/free.yaml"/>
    <param name="sensors" value="$(find cob_robot_config)/$(env ROBOT)/user_defined/sensors.yaml"/>
    <param name="minimal_system" value="$(find cob_robot_config)/common/minimal_system.yaml"/>

</launch>
